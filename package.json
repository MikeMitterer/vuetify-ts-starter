{
  "name": "@mmit/vuetify-ts-starter",
  "version": "0.2.1",
  "private": true,
  "description": "VUETIFY/TS Starter-Template",
  "type": "module",
  "author": "Mike Mitterer",
  "scripts": {
    "pkg:version": "echo $(jq -r -j '.name' < package.json;echo -n ': ';jq -r -j '.version' < package.json)",
    "clean": "rm -rf lib dist bin && jest --clearCache",
    "update:browser": "browserslist --update-db",
    "download:certs": "rm -rf .ssl && cert2ssl.sh -d int.mikemitterer.at",
    "build": "vue-cli-service build --mode development",
    "test:unit": "jest --colors test/unit",
    "lint": "tslint --project tsconfig.json",
    "deploy": "yarn version --new-version patch",
    "build:prod": "vue-cli-service build --mode production",
    "start": "vite --mode development --open",
    "start:prod": "vite --mode production --open",
    "tc": "tsc --noEmit",
    "test": "yarn run test:unit",
    "test:integration": "jest --colors --runInBand --detectOpenHandles test/integration"
  },
  "keywords": [
    "typescript vue"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeMitterer/vuetify-ts-starter"
  },
  "dependencies": {
    "@mmit/logging": "0.1.8",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.7.1",
    "uuid": "^8.3.2",
    "vue": "^2.7",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.6.5",
    "vuetify": "^2.5.14",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/plugin-transform-runtime": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.22.11",
    "@mdi/font": "^6.3.95",
    "@types/jest": "29",
    "@types/uuid": "^8.3.1",
    "@types/websocket": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitejs/plugin-vue2": "2.2.0",
    "@vue/test-utils": "1",
    "@vue/vue2-jest": "29.2.4",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "date-fns": "2.23.0",
    "eslint": "8",
    "eslint-plugin-vue": "9.17.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "2.0.0",
    "moment": "^2.29.1",
    "sass": "~1.32",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tslint": "^6.1.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-ejs": "1.6.4",
    "vite-plugin-node-polyfills": "0.15.0",
    "vite-plugin-pwa": "^0.16.5",
    "vue-template-compiler": "2.7",
    "vue-tsc": "1.8.15",
    "vuetify2-component-types": "2.7",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1"
  }
}
