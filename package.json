{
    "name": "@mmit/vuetify-ts-starter",
    "version": "0.1.12",
    "author": "Mike Mitterer",
    "description": "VUETIFY/TS Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit tests/unit/.*",
        "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch",
        "clean": "rm -rf lib dist bin doc && jest --clearCache"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/vuetify-ts-starter"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.0",
        "vue": "^2.6.11",
        "vue-class-component": "^7.2.5",
        "vue-property-decorator": "^9.0.0",
        "vue-router": "^3.3.4",
        "vuetify": "^2.3.7",
        "vuetify-loader": "^1.6.0",
        "vuex": "^3.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-typescript": "^7.10.4",
        "@mdi/font": "^5.4.55",
        "@types/jest": "26.0.20",
        "@types/uuid": "^8.0.0",
        "@vue/cli-plugin-typescript": "^4.4.6",
        "@vue/cli-plugin-unit-jest": "^4.4.6",
        "@vue/cli-service": "^4.4.6",
        "@vue/test-utils": "1.1.2",
        "babel-jest": "26.6.3",
        "jest": "26.6.3",
        "jest-extended": "^0.11.5",
        "moment": "^2.27.0",
        "sass": "^1.26.10",
        "sass-loader": "10.1.1",
        "ts-jest": "26",
        "typescript": "^4.1.3",
        "vue-cli-plugin-vuetify": "^2.0.7",
        "vue-jest": "^4.0.0-beta.2",
        "vue-template-compiler": "^2.6.11",
        "vuex-class": "^0.3.2",
        "vuex-module-decorators": "^1.0.1"
    }
}
