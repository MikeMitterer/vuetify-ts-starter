{
    "name": "@mmit/vuetify-ts-starter",
    "version": "0.1.11",
    "author": "Mike Mitterer",
    "description": "VUETIFY/TS Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit tests/unit/.*",
        "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "uuid": "^3.3.3",
        "vue": "^2.6.10",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.3.0",
        "vue-router": "^3.1.3",
        "vuetify": "^2.1.5",
        "vuetify-loader": "^1.3.0",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-typescript": "^7.6.0",
        "@mdi/font": "^4.5.95",
        "@types/jest": "24",
        "@types/uuid": "^3.4.5",
        "@vue/cli-plugin-typescript": "^4.0.4",
        "@vue/cli-plugin-unit-jest": "^4.0.4",
        "@vue/cli-service": "^4.0.4",
        "@vue/test-utils": "1.0.0-beta.29",
        "babel-jest": "24",
        "jest": "24",
        "jest-extended": "^0.11.2",
        "moment": "^2.24.0",
        "sass": "^1.23.0",
        "sass-loader": "8.0.0",
        "ts-jest": "24",
        "typescript": "^3.6.4",
        "vue-cli-plugin-vuetify": "^1.1.0",
        "vue-jest": "^4.0.0-beta.2",
        "vue-template-compiler": "^2.6.10",
        "vuex-class": "^0.3.2",
        "vuex-module-decorators": "^0.10.1"
    },
    "resolutions": {
        "@vue/cli-plugin-unit-jest/jest": "24",
        "@vue/cli-plugin-unit-jest/babel-jest": "24"
    }
}
