{
    "name": "@mmit/vuetify-ts-starter",
    "version": "0.1.12",
    "author": "Mike Mitterer",
    "description": "VUETIFY/TS Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "vue-cli-service test:unit tests/unit/.*",
        "test:integration": "vue-cli-service test:unit --runInBand --detectOpenHandles tests/integration/.*",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch",
        "clean": "rm -rf lib dist bin && jest --clearCache"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/vuetify-ts-starter"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.5",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2",
        "vue": "^2.6.12",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.5.1",
        "vuetify": "^2.4.8",
        "vuetify-loader": "^1.7.2",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.13.10",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-typescript": "^7.13.0",
        "@mdi/font": "^5.9.55",
        "@types/jest": "26.0.21",
        "@types/uuid": "^8.3.0",
        "@types/websocket": "^1.0.2",
        "@vue/cli-plugin-babel": "^4.5.12",
        "@vue/cli-plugin-typescript": "^4.5.12",
        "@vue/cli-plugin-unit-jest": "^4.5.12",
        "@vue/cli-service": "^4.5.12",
        "@vue/test-utils": "1.1.3",
        "babel-jest": "26.6.3",
        "babel-loader": "^8.2.2",
        "jest": "26.6.3",
        "jest-extended": "^0.11.5",
        "moment": "^2.29.1",
        "sass": "^1.32.8",
        "sass-loader": "10.1.1",
        "ts-jest": "26.5.4",
        "typescript": "^4.2.3",
        "vue-cli-plugin-vuetify": "^2.3.1",
        "vue-jest": "^4.0.0-beta.2",
        "vue-template-compiler": "^2.6.12",
        "vuex-class": "^0.3.2",
        "vuex-module-decorators": "^1.0.1"
    }
}
