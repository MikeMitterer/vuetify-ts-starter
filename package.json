{
  "name": "@mmit/vuetify-ts-starter",
  "version": "0.3.0",
  "private": true,
  "description": "VUETIFY/TS Starter-Template",
  "type": "module",
  "author": "Mike Mitterer",
  "scripts": {
    "version:gen": "package2json.sh -g",
    "version:pkg": "jq -r '.version' < package.json",
    "version:pkg:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:pkg)",
    "version:remote": "curl --silent -X GET -H \"Authorization: token ${GH_TOKEN_READ_PRIVATE_REPO}\" \"https://raw.githubusercontent.com/$(cat ./.gitorg)/$(git repo)/master/package.json\" | jq -r \".version\" ",
    "version:remote:full": "echo $(jq -r -j '.name' < package.json;echo ': ';yarn version:remote)",
    "update": "yarn update:browser",
    "update:browser": "browserslist --update-db",
    "update:templates": "updateDevTemplates.sh -u",
    "download:certs": "rm -rf .ssl && cert2ssl.sh -d int.mikemitterer.at",
    "clean": "rm -rf lib dist bin",
    "clean:jest": "jest --clearCache",
    "clean:all": "yarn clean && yarn clean:jest && rm -f yarn.lock && rm -rf node_modules && yarn clean cache",
    "start": "vite --mode development --open",
    "start:live": "vite --mode live --open",
    "start:prod": "vite --mode production --open",
    "tc": "tsc --noEmit --project tsconfig.json",
    "lint": "yarn eslint src tests",
    "lint:fix": "yarn eslint src --fix",
    "test": "yarn run test:unit",
    "test:unit": "jest --colors tests/unit",
    "test:components": "jest --colors tests/components",
    "test:integration": "jest --colors --runInBand --detectOpenHandles tests/integration",
    "test:e2e": "E2E_TEST='true' jest --colors --runInBand --detectOpenHandles tests/e2e",
    "build": "yarn run --silent version:gen dev > public/app.json && vue-tsc --noEmit && vite --mode development build",
    "build:types": "vue-tsc --emitDeclarationOnly && resolve-tspaths",
    "build:live": "yarn run --silent version:gen live > public/app.json && vue-tsc --noEmit && vite --mode live build",
    "build:prod": "yarn run --silent version:gen production > public/app.json && vue-tsc --noEmit && vite --mode production build",
    "deploy": "test $(yarn --version | awk -F. '{ print $1 }') -eq 4 && yarn run deploy:y4 || yarn run deploy:y2",
    "deploy:y2": "yarn version --new-version patch && yarn publish --non-interactive",
    "deploy:y4": "yarn version patch && yarn npm publish --access restricted",
    "export": "yarn export:scripts",
    "export:scripts": "jq -r '.scripts' package.json > \"${DEV_LOCAL}/Templates/Production/TypeScript/package/scripts-vite_env.json\""
  },
  "keywords": [
    "typescript vue"
  ],
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/MikeMitterer/vuetify-ts-starter"
  },
  "dependencies": {
    "@mmit/logging": "0.1.15",
    "reflect-metadata": "^0.1.13",
    "register-service-worker": "^1.7.1",
    "uuid": "^8.3.2",
    "vue": "2.7.13",
    "vue-class-component": "7.2.6",
    "vue-property-decorator": "9.1.2",
    "vue-router": "3.5.4",
    "vuetify": "2.7.1",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
    "@mdi/font": "^6.3.95",
    "@types/jest": "29.5.4",
    "@types/uuid": "^8.3.1",
    "@types/websocket": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitejs/plugin-vue2": "2.2.0",
    "@vue/test-utils": "1",
    "@vue/vue2-jest": "29.2.4",
    "babel-jest": "29.7.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "date-fns": "2.23.0",
    "eslint": "8.50.0",
    "eslint-plugin-vue": "9.17.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-environment-node": "29.7.0",
    "jest-extended": "2.0.0",
    "moment": "^2.29.1",
    "sass": "~1.32",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "4.5.2",
    "vite-plugin-ejs": "1.6.4",
    "vite-plugin-node-polyfills": "0.15.0",
    "vite-plugin-pwa": "0.16.5",
    "vue-template-compiler": "2.7.13",
    "vue-tsc": "1.8.15",
    "vuetify2-component-types": "2.7",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1"
  }
}
