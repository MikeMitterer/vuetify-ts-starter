{
    "name": "@mmit/vuetify-ts-starter",
    "version": "0.1.12",
    "author": "Mike Mitterer",
    "description": "VUETIFY/TS Starter-Template",
    "keywords": [
        "typescript vue"
    ],
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "clean": "rm -rf lib dist bin &&  rm -rf node_modules/@types/istanbul-reports && jest --clearCache && rm -rf .ts.cache && rm -rf .rollup.cache && rm -f .tsbuildinfo",
        "start": "vue-cli-service serve --mode development --open",
        "tc": "tsc --noEmit",
        "lint": "vue-cli-service lint",
        "test": "yarn run test:unit",
        "test:unit": "jest --colors test/unit",
        "test:integration": "jest --colors --runInBand --detectOpenHandles test/integration",
        "build": "vue-cli-service build --mode development",
        "build:prod": "vue-cli-service build --mode production",
        "deploy": "yarn version --new-version patch"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MikeMitterer/vuetify-ts-starter"
    },
    "dependencies": {
        "@mmit/logging": "^0.1.8",
        "reflect-metadata": "^0.1.13",
        "uuid": "^8.3.2",
        "vue": "^2.6.14",
        "vue-class-component": "^7.2.6",
        "vue-property-decorator": "^9.1.2",
        "vue-router": "^3.5.2",
        "vuetify": "^2.5.6",
        "vuetify-loader": "^1.7.2",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@mdi/font": "^5.9.55",
        "@types/jest": "26.0.24",
        "@types/uuid": "^8.3.1",
        "@types/websocket": "^1.0.3",
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-plugin-typescript": "^4.5.13",
        "@vue/cli-plugin-unit-jest": "^4.5.13",
        "@vue/cli-service": "^4.5.13",
        "@vue/test-utils": "1.2.1",
        "babel-jest": "26",
        "babel-loader": "^8.2.2",
        "jest": "26",
        "jest-extended": "^0.11.5",
        "jest-transform-stub": "^2.0.0",
        "moment": "^2.29.1",
        "sass": "^1.32.8",
        "sass-loader": "10.1.1",
        "ts-jest": "26",
        "typescript": "^4.3.5",
        "vue-cli-plugin-vuetify": "^2.4.1",
        "vue-jest": "4",
        "vue-template-compiler": "^2.6.14",
        "vuex-class": "^0.3.2",
        "vuex-module-decorators": "^1.0.1"
    }
}
